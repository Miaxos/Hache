# include <stdio.h>
# include <stdlib.h>
# include <string.h>

# define TAILLE_MAX 30

int isFunction(const char *function) {
	return strcmp(function,"myls");
}

char* getFunction(const char *function, char *function2) {
	char c;
	int compteur = 0;
	while ((c=function[compteur]) != ' ' && (c=function[compteur]) != '\n') {
		printf("%i\n", compteur);
		function2[compteur++] = c;
	}
	return function2;
}

int callFunction(const char *function) {
	char function2[TAILLE_MAX];
	sprintf(function2, "../functions/%s", function);
	int status = system(function2);
	return status;
}
